<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="320dp"
        android:layout_height="31dp"
        android:layout_marginStart="72dp"
        android:layout_marginLeft="72dp"
        android:layout_marginTop="100dp"
        android:orientation="horizontal"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <ImageView
            android:id="@+id/student_place_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginRight="18.7dp"
            android:paddingLeft="19dp"
            android:src="@drawable/domain_24_px_1"
            app:layout_constraintEnd_toStartOf="@+id/store_list"
            tools:layout_editor_absoluteY="137dp" />

        <TextView
            android:id="@+id/store_list"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nanum_square_round_r"
            android:lineSpacingExtra="2sp"
            android:textColor="#656975"
            android:textSize="16sp"
            android:textStyle="normal"
            tools:layout_editor_absoluteX="110dp"
            tools:layout_editor_absoluteY="136dp"
            tools:text="학문관" />


        <CheckBox
            android:id="@+id/store_check"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="177.3dp"
            android:checked="true"
            app:layout_constraintStart_toEndOf="@+id/store_list"
            tools:layout_editor_absoluteY="131dp" />

    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>
